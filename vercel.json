{
   "version": 2,
   "name": "mern_ecommerce",
   "builds": [
      {
         "src": "server.js",
         "use": "@vercel/node"
      },
      {
         "src": "/ui_reactjs/build/*",
         "use": "@now/static"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/server.js"
      }
   ]
}
